    "@types/nedb": "1.8.7",
    "@types/node": "11.10.4",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/styled-components": "4.1.11",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",

    "typescript": "3.2.2",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "axios": "0.18.0",
    "electron-updater": "4.0.6",
    "gsap": "2.0.2",
    "leveldown": "4.0.1",
    "levelup": "4.0.0",
    "mobx": "5.8.0",
    "mobx-react": "5.4.3",
    "mousetrap": "1.6.2",
    "nedb": "1.8.0",
    "react": "16.7.0",

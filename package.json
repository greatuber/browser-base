{
  "name": "wexond",
  "version": "2.2.3",
  "description": "Extensible, fast and innovative web browser with material UI.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/wexond/wexond#readme",
  "main": "build/main.bundle.js",
  "author": {
    "name": "Eryk Rakowski",
    "email": "sentialx@gmail.com"
  },
  "contributors": [
    {
      "name": "Eryk Rakowski",
      "email": "sentialx@gmail.com"
    },
    {
      "name": "Miko≈Çaj Palkiewicz",
      "email": "xnerhu@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wexond/wexond.git"
  },
  "bugs": {
    "url": "https://github.com/wexond/wexond/issues"
  },
  "scripts": {
    "dev": "concurrently \"webpack-dev-server --config webpack.config.renderer.dev.js\" \"webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.prod.js\" \"webpack --config webpack.config.prod.js\"",
    "build-dll": "webpack --config webpack.config.vendor.js --mode production",
    "build-dll-dev": "webpack --config webpack.config.vendor.js --mode development",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps",
    "build-ci": "node build.js"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "devDependencies": {
    "@cliqz/adblocker": "0.12.0",
    "@types/axios": "0.14.0",
    "@types/chrome": "0.0.87",
    "@types/gsap": "1.20.2",
    "@types/nedb": "1.8.8",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/styled-components": "4.1.18",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "axios": "0.19.0",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.1",
    "cross-env": "5.2.0",
    "electron": "^7.0.0-beta.1",
    "electron-builder": "21.2.0",
    "electron-react-devtools": "0.5.3",
    "electron-updater": "4.1.2",
    "file-type": "12.1.0",
    "fuse-box": "3.7.1",
    "gsap": "2.1.3",
    "html-webpack-plugin": "^3.2.0",
    "icojs": "0.13.1",
    "mobx": "5.13.0",
    "mobx-react-lite": "1.4.1",
    "nedb": "1.8.0",
    "node-bookmarks-parser": "^2.0.0",
    "prettier": "1.18.2",
    "pretty-bytes": "5.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.10",
    "react-windows-controls": "1.0.5",
    "styled-components": "4.3.2",
    "terser": "4.1.3",
    "tldts-experimental": "5.3.2",
    "ts-loader": "^6.0.4",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "typescript-plugin-styled-components": "1.4.3",
    "url-loader": "^2.1.0",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "extract-file-icon": "0.3.1",
    "mouse-hooks": "0.4.1",
    "node-vibrant": "3.2.0-alpha",
    "node-window-manager": "1.0.4",
    "electron-extensions": "2.0.2"
  }
}

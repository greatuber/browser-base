{
  "name": "wexond",
  "version": "4.0.0-beta.2",
  "description": "Extensible, fast and innovative web browser with material UI.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/wexond/wexond#readme",
  "main": "build/main.bundle.js",
  "author": "Eryk Rakowski <sentialx@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wexond/wexond.git"
  },
  "bugs": {
    "url": "https://github.com/wexond/wexond/issues"
  },
  "scripts": {
    "dev": "cross-env START=1 npm run watch",
    "build": "npm run extensions && concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\" \"webpack --config webpack.config.web.js\"",
    "start": "cross-env NODE_ENV='dev' electron .",
    "watch": "npm run extensions && concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\" \"cross-env ENV='dev' webpack-dev-server --config webpack.config.web.js\"",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "compile": "node scripts/compile.js",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "electron-rebuild -f -o keytar",
    "extensions": "node scripts/extensions.js"
  },
  "devDependencies": {
    "@cliqz/adblocker-electron": "1.4.1",
    "@types/chrome": "0.0.91",
    "@types/crypto-js": "^3.1.43",
    "@types/extract-zip": "^1.6.2",
    "@types/gsap": "1.20.2",
    "@types/jszip": "^3.1.7",
    "@types/mkdirp": "^0.5.2",
    "@types/nedb": "1.8.9",
    "@types/node": "13.1.6",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "awesome-node-loader": "^1.1.1",
    "axios": "0.19.1",
    "cache-loader": "^4.1.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "6.0.3",
    "crypto-js": "^3.1.9-1",
    "electron": "8.0.0-beta.6",
    "electron-builder": "21.2.0",
    "electron-extensions": "^6.0.1",
    "electron-rebuild": "^1.8.8",
    "electron-updater": "4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "extract-zip": "^1.6.7",
    "file-loader": "^5.0.2",
    "file-type": "13.0.3",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gsap": "^3.0.5",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "icojs": "^0.14.0",
    "jszip": "^3.2.2",
    "keytar": "^5.0.0",
    "mobx": "5.15.2",
    "mobx-react-lite": "1.5.2",
    "nedb": "1.8.0",
    "node-bookmarks-parser": "^2.0.0",
    "node-fetch": "^2.6.0",
    "node-vibrant": "^3.1.5",
    "prettier": "1.19.1",
    "pretty-bytes": "5.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.18",
    "react-windows-controls": "1.1.1",
    "styled-components": "^5.0.0",
    "terser": "^4.6.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {}
}
